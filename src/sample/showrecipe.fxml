<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="405.0" prefWidth="505.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="recipe.Showrecipe">
   <children>
      <TableView fx:id="recipeTable" layoutX="148.0" layoutY="117.0" prefHeight="288.0" prefWidth="231.0">
         <columns>
            <TableColumn fx:id="idCol" prefWidth="75.0" text="Id" />
            <TableColumn fx:id="nameCol" prefWidth="75.0" text="Name" />
            <TableColumn fx:id="priceCol" prefWidth="75.0" text="Price" />
         </columns>
      </TableView>
      <Label alignment="CENTER" layoutX="47.0" layoutY="39.0" prefHeight="18.0" prefWidth="456.0" text="Recipe Table">
         <font>
            <Font size="34.0" />
         </font>
      </Label>
      <Button fx:id="showrecipe" layoutX="24.0" layoutY="51.0" mnemonicParsing="false" onAction="#search" prefHeight="26.0" prefWidth="79.0" text="Show" />
      <Button fx:id="backTable" layoutX="440.0" layoutY="51.0" mnemonicParsing="false" onAction="#switchToEmployeeMenu" prefHeight="26.0" prefWidth="52.0" text="Back" />
   </children>
</AnchorPane>

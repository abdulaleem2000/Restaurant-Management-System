        </columns>
      </TableView>
      <Label alignment="CENTER" layoutX="-1.0" layoutY="52.0" prefHeight="18.0" prefWidth="600.0" text="Order table" />
      <Button fx:id="showOrderTable" layoutX="56.0" layoutY="49.0" mnemonicParsing="false" onAction="#search" text="Show" />
      <Button fx:id="backoRDERtABLE" layoutX="503.0" layoutY="49.0" mnemonicParsing="false" onAction="#SwitchToOrderMenu" text="Back" />
   </children>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Showattend">
   <children>
      <TableView fx:id="attView" layoutX="156.0" layoutY="123.0" prefHeight="263.0" prefWidth="322.0">
        <columns>
          <TableColumn fx:id="datecolAtt" prefWidth="75.0" text="date" />
          <TableColumn fx:id="attCol" prefWidth="75.0" text="attendance" />
            <TableColumn fx:id="idattCol" prefWidth="75.0" text="idemployee" />
        </columns>
      </TableView>
      <Button fx:id="showAtt" layoutX="53.0" layoutY="52.0" mnemonicParsing="false" onAction="#search" text="show" />
      <Button fx:id="backAtt" layoutX="514.0" layoutY="52.0" mnemonicParsing="false" onAction="#SwitchToAttendance" text="back" />
   </children>
</AnchorPane>

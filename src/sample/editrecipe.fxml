         <font>
            <Font size="31.0" />
         </font>
      </Label>
      <TextField fx:id="editRecipe" layoutX="165.0" layoutY="111.0" />
      <Text layoutX="66.0" layoutY="128.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Id of Recipe:" />
      <Button fx:id="backEdit" layoutX="501.0" layoutY="361.0" mnemonicParsing="false" onAction="#switchToRecipeMenu" prefHeight="26.0" prefWidth="77.0" text="Back" />
      <Button fx:id="editRecipeButton" layoutX="26.0" layoutY="361.0" mnemonicParsing="false" onAction="#editRecipe" prefHeight="26.0" prefWidth="94.0" text="Edit" />
   </children>
</AnchorPane>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <TextField fx:id="attendanceId" layoutX="250.0" layoutY="151.0" prefHeight="26.0" prefWidth="175.0" />
      <Text layoutX="147.0" layoutY="168.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter Student Id:" />
      <Button fx:id="present" layoutX="207.0" layoutY="280.0" mnemonicParsing="false" onAction="#markPresent" prefHeight="26.0" prefWidth="88.0" text="Present" />
      <Button fx:id="absent" layoutX="387.0" layoutY="280.0" mnemonicParsing="false" onAction="#markAbsent" prefHeight="26.0" prefWidth="81.0" text="Absent" />
      <Button fx:id="backAtt" layoutX="488.0" layoutY="360.0" mnemonicParsing="false" onAction="#switchToAttendance" prefHeight="26.0" prefWidth="62.0" text="back" />
      <DatePicker fx:id="attDate" layoutX="250.0" layoutY="210.0" />
      <Text layoutX="169.0" layoutY="227.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Select Date:" />
   </children>
</AnchorPane>

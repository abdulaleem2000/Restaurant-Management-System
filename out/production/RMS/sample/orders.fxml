<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <Button fx:id="takeOrderButton" layoutX="245.0" layoutY="135.0" mnemonicParsing="false" onAction="#orderId" prefHeight="26.0" prefWidth="110.0" text="Take Orders" />
      <Button fx:id="seeOrdersButton" layoutX="245.0" layoutY="208.0" mnemonicParsing="false" onAction="#SwitchToShowOrders" prefHeight="26.0" prefWidth="110.0" text="See orders" />
      <Button fx:id="backOrders" layoutX="517.0" layoutY="352.0" mnemonicParsing="false" onAction="#switchToMainMenu" prefHeight="26.0" prefWidth="65.0" text="Back" />
      <Label alignment="CENTER" layoutY="60.0" prefHeight="52.0" prefWidth="600.0" text="Order Management">
         <font>
            <Font size="27.0" />
         </font>
      </Label>
   </children>
</AnchorPane>

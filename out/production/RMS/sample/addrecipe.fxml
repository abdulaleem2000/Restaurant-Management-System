<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="762.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <TextField fx:id="nameRecipe" layoutX="152.0" layoutY="89.0" prefHeight="26.0" prefWidth="175.0" />
      <TextField fx:id="idRecipe" layoutX="152.0" layoutY="149.0" prefHeight="26.0" prefWidth="175.0" promptText="unique" />
      <Label fx:id="lnRecipe" layoutX="24.0" layoutY="92.0" text="Name of Recipe:" />
      <Label fx:id="lnID" layoutX="22.0" layoutY="152.0" text="Id of Recipe:" />
      <Label fx:id="addRecipe" alignment="CENTER" layoutX="-4.0" layoutY="-1.0" prefHeight="78.0" prefWidth="770.0" text="Add Recipe">
         <font>
            <Font size="38.0" />
         </font>
      </Label>
      <Button fx:id="backAddRecipe" layoutX="648.0" layoutY="361.0" mnemonicParsing="false" onAction="#switchToRecipeMenu" prefHeight="30.0" prefWidth="70.0" text="Back" />
      <Button fx:id="addRec" layoutX="470.0" layoutY="155.0" mnemonicParsing="false" onAction="#addRecipe" prefHeight="78.0" prefWidth="128.0" text="Add" />
      <TextField fx:id="priceRecipeText" layoutX="152.0" layoutY="206.0" prefHeight="26.0" prefWidth="175.0" />
      <Label fx:id="priceRecipe" layoutX="22.0" layoutY="210.0" text="Price of Recipe" />
   </children>
</AnchorPane>
